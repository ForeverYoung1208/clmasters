{
  "name": "clmasters",
  "version": "1.0.0",
  "description": "clock masters management",
  "main": "build/app.ts",
  "scripts": {
    "server": "nodemon --config nodemon.json --inspect src/app.ts",
    "dev": "NODE_ENV=development concurrently \"yarn server\" \"yarn client\"",
    "build": "NODE_ENV=production tsx src/app.js",
    "prod": "NODE_ENV=production node app.js",
    "client": "yarn --cwd ../client start",
    "client:install": "yarn --cwd ../client install",
    "client:build": "yarn --cwd ../client build",
    "test:unit": "ENV_FILE=.env.test jest --detectOpenHandles"
  },
  "repository": "git@github.com:ForeverYoung1208/clmasters.git",
  "author": "Igor <siafin2010@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "add": "^2.0.6",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "googleapis": "^73.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-mailjet": "^3.3.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3",
    "stripe": "^8.154.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/supertest": "^2.0.11",
    "concurrently": "^5.2.0",
    "eslint": "^7.31.0",
    "eslint-plugin-jest": "^24.3.6",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.4",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
